package edu.elon.math;

public class CalcModel implements ModelInterface {
	double firstNum;
	double secondNum;
	String op;
	

	@Override
	public void setFirstNum(String num) {
		double dNum = Double.parseDouble(num);
		firstNum = dNum;
		
	}

	@Override
	public void setOp(String op) {
		this.op = op;
	}

	@Override
	public void setSecondNum(String num) {
		double dNum = Double.parseDouble(num);
		secondNum = dNum;
		
	}

}
